				<div class="{style.formfield}">
					<g:TextBox ui:field='compassionField' width="25em">100</g:TextBox>
				</div>
			</div>
			<div class="{style.formwrapper}">
				<div class="{style.formlabel}">
					<g:Label> Joy (%)</g:Label>
				</div>
				<div class="{style.formfield}">
					<g:TextBox ui:field='joyField' width="25em">100</g:TextBox>
				</div>
			</div>
			<div class="{style.formwrapper}">
				<div class="{style.formlabel}">
					<g:Label> Hope of Future (%)</g:Label>
				</div>
				<div class="{style.formfield}">
					<g:TextBox ui:field='hopeField' width="25em">100</g:TextBox>
				</div>
			</div>
		<div class='{style.button}'>
			<g:Anchor href='javascript:;' ui:field='saveBut'>Save</g:Anchor>
			<g:Anchor href='javascript:;' ui:field='cancelBut'>Cancel</g:Anchor>
		</div>
		<div ui:field='nameDiv' />
	</g:HTMLPanel>
</ui:UiBinder> 