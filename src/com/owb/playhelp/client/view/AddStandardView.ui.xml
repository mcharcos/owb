<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style src="../resources/GlobalStyles.css" >
	</ui:style>
	<g:HTMLPanel>
			<div class="{style.formTitle}">Status</div>
			<!-- 
			<div class="{style.formwrapper}">
				<div class="{style.formlabel}">
					<g:Label ui:field='nameField'>Name</g:Label>
				</div>
			</div>
			 -->
			<div class="{style.formwrapper}">
				<div class="{style.formlabel}">
					<g:Label> Clean Water (%)</g:Label>
				</div>
				<div class="{style.formfield}">
					<g:TextBox ui:field='waterField' width="25em">100</g:TextBox>
				</div>
			</div>
			<div class="{style.formwrapper}">
				<div class="{style.formlabel}">
					<g:Label> Food (%)</g:Label>
				</div>
				<div class="{style.formfield}">
					<g:TextBox ui:field='foodField' width="25em">100</g:TextBox>
				</div>
			</div>
			<div class="{style.formwrapper}">
				<div class="{style.formlabel}">
					<g:Label> Shelter (%)</g:Label>
				</div>
				<div class="{style.formfield}">
					<g:TextBox ui:field='shelterField' width="25em">100</g:TextBox>
				</div>
			</div>
			<div class="{style.formwrapper}">
				<div class="{style.formlabel}">
					<g:Label> Clothing (%)</g:Label>
				</div>
				<div class="{style.formfield}">
					<g:TextBox ui:field='clothingField' width="25em">100</g:TextBox>
				</div>
			</div>
			<div class="{style.formwrapper}">
				<div class="{style.formlabel}">
					<g:Label> Medicine (%)</g:Label>
				</div>
				<div class="{style.formfield}">
					<g:TextBox ui:field='medicineField' width="25em">100</g:TextBox>
				</div>
			</div>
			<div class="{style.formwrapper}">
				<div class="{style.formlabel}">
					<g:Label> Hygiene (%)</g:Label>
				</div>
				<div class="{style.formfield}">
					<g:TextBox ui:field='hygieneField' width="25em">100</g:TextBox>
				</div>
			</div>
			<div class="{style.formwrapper}">
				<div class="{style.formlabel}">
					<g:Label> Safety (%)</g:Label>
				</div>
				<div class="{style.formfield}">
					<g:TextBox ui:field='safetyField' width="25em">100</g:TextBox>
				</div>
			</div>
			<div class="{style.formwrapper}">
				<div class="{style.formlabel}">
					<g:Label> Activity (%)</g:Label>
				</div>
				<div class="{style.formfield}">
					<g:TextBox ui:field='activityField' width="25em">100</g:TextBox>
				</div>
