			<div class="{style.formwrapper}">
				<div class="{style.formlabel}">
					<g:Label> Education (%)</g:Label>
				</div>
				<div class="{style.formfield}">
					<g:TextBox ui:field='educationField' width="25em">100</g:TextBox>
				</div>
			</div>
			<div class="{style.formwrapper}">
				<div class="{style.formlabel}">
					<g:Label> Guidance (%)</g:Label>
				</div>
				<div class="{style.formfield}">
					<g:TextBox ui:field='guidanceField' width="25em">100</g:TextBox>
				</div>
			</div>
			<div class="{style.formwrapper}">
				<div class="{style.formlabel}">
					<g:Label> Responsibility (%)</g:Label>
