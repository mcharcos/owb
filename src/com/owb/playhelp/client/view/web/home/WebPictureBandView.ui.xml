<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	
	<ui:style>
		.important {
			font-weight: bold;
		}
		.flowPanel_inline
		{
		    display:inline-block;
		}
	</ui:style>
	<g:FlowPanel styleName="{style.flowPanel_inline}">
	  <g:Label ui:field='nextLeft'>Previous</g:Label>
	  <g:DeckLayoutPanel ui:field='panelSlider'/>
	  <g:Label ui:field='nextRight'>Next</g:Label>  
	</g:FlowPanel>
</ui:UiBinder> 