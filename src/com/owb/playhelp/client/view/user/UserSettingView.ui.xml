<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	
    <ui:with field='res' type='com.owb.playhelp.client.resources.Resources'/>
    
	<ui:style src="../../resources/GlobalStyles.css">
		.important {
			font-weight: bold;
		}
		.username {
			text-align: left;
		}
		.backgroundall {  
	        background-color: #8bff2d;
			height                      :   60px;
         }  
	</ui:style>
	<g:HorizontalPanel  styleName='{style.backgroundall}'>
	  <g:Image resource='{res.settingIcon}'  ui:field='preferences'/>
	   <g:Image resource='{res.bar3Logo}' width='10px'/>
	  <g:Image resource='{res.loginIcon}'  ui:field='loginout'/>
	</g:HorizontalPanel>
</ui:UiBinder> 